
<div class="page-wrapper">
	<header class="fixed-top">
		<nav class="navbar navbar-expand-lg">
			<div class="container">
		
				<!-- Logo -->
			
					{{ page.header }}
				
				<!-- logo End -->
		
				{% if page.primary_menu %}
		
					<button class="navbar-toggler d-lg-none" type="button"
						data-toggle="collapse" data-target="#divNavbar"
						aria-controls="divNavbar" aria-expanded="false"
						aria-label="Toggle navigation">
						
						<span class="navbar-toggler-icon">Menu</span>
					</button>
				
					<!-- Navigation -->
		  
					<div id="divNavbar"
						class="main-navigation navigation navbar-collapse collapse"
						role="navigation" aria-label="Main menu">
						<div class="menu-main-menu-container">
							{{ page.primary_menu }}
						</div>
					</div>
					
					<!-- end Navigation -->
		  
				{% endif %}
		
			</div>
		</nav>
	</header>
  
    <!-- main page content -->
	<main role="main">
		{% block content %}
			{{ page.content }}
		{% endblock %}
	</main>
	<!-- end main page content -->
	
	
	<footer>
	<!-- footer section -->
		<div class="footer-info">
			<img src="{{ base_url }}/{{ directory }}/img/0.gif" class="div-img"
				style="background-image:url( {{ base_path ~ directory }}/img/footer-bg.jpg);height:663px;width:1423px;"
				alt="Footer Logo">
				
				<div class="{{ container }}">
					<div class="row">
						<div class="col-sm-6 col-md-6  col-lg-3">

							<a href="{{ base_url }}" class="footer-logo">
								<img src="{{ base_url }}/{{ directory }}/img/footer-logo.png"
								width="63" height="64" alt="footer-logo">
							</a>

							{% block footer_first %}
								{% if page.footer_first %}
									{{ page.footer_first }}
								{% endif %}
							{% endblock %}
						</div>
						<div class="col-sm-6 col-md-6  col-lg-3">
							{% block footer_second %}
								{% if page.footer_second %}
									{{ page.footer_second }}
								{% endif %}
							{% endblock %}
						</div>
						<div class="col-sm-6 col-md-6  col-lg-3">
							{% block footer_third %}
								{% if page.footer_third %}
									{{ page.footer_third }}
								{% endif %}
							{% endblock %}
						</div>
						<div class="col-sm-6 col-md-6  col-lg-3">
							{% block footer_fourth %}
								{% if page.footer_fourth %}
									{{ page.footer_fourth }}
								{% endif %}
							{% endblock %}
						</div>
					</div>
				</div>
		</div>
		{% block footer_fifth %}
			<div class="copyright">
				<div class="container" {{ attributes }}>
					<div class="row">
						<div class="col-md-6 copy-txt">
							Â© {{ 'now'|date('Y') }} Kitchens Kelley Gaynes, P.C.&nbsp; | &nbsp;<a href="{{ base_url }}/privacy-policy">Privacy Policy</a>&nbsp; | &nbsp;<a href="{{ base_url }}/terms-use">Terms of Use</a>&nbsp; | &nbsp;<a href="{{ base_url }}/disclaimer">Disclaimer </a>
						</div>
						<div class="col-md-6 text-center text-md-right pt-2 pt-md-0">
							<a href="https://www.spinxdigital.com/" rel="noopener"
							target="_blank">Design by SPINX Digital</a>
						</div>
					</div>
				</div>
			</div>
		
		{% endblock %}
	</footer>
	<!-- footer section end here -->
</div>
