<section class="related-attor attorneys-sec practice-sec related-attorney">
		<div class="container" {{ attributes }}>
			<div class="row">
				<div class="sec-title text-center">
					{% if header %}
						{{ header }}
					{% endif %}
				</div>
            </div>
			<div class="row justify-content-end mb-5">
				<div class="col-md-8 col-lg-5 mx-xl-0 mx-auto">
					<div class="form-label cmn-select">
						{% if exposed %}
							<div class="view-filters">
								{{ exposed }}
							</div>
						{% endif %}
                    </div>
				</div>
            </div>
		    {% if rows %}
			<div class="row area-list attor-no-slide">
				{{ rows }}
				{% elseif empty %}
					<div class="view-empty">
						{{ empty }}
					</div>
			{% endif %}
			</div>
		</div>
</section>

      
    